{
  "name": "catkin",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "npm run serve",
    "serve": "NODE_ENV=production node -r dotenv/config ./index.js",
    "serve:static": "serve ./build",
    "develop": "NODE_ENV=development node -r dotenv/config ./index.js",
    "storybook": "start-storybook -p 6006 -c ./config/storybook",
    "build:client": "mkdir -p build && NODE_ENV=production webpack -p --config ./config/webpack.client.js --json > ./build/stats.json",
    "build:server": "npm run build:client && NODE_ENV=production webpack -p --config ./config/webpack.server.js",
    "build:static": "npm run build:client && npm run build:server && NODE_ENV=production webpack -p --config ./config/webpack.static.js",
    "build:storybook": "build-storybook",
    "test": "jest",
    "test:cover": "jest --coverage"
  },
  "devDependencies": {
    "@storybook/addon-knobs": "^3.2.8",
    "@storybook/react": "^3.2.8",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^21.0.2",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.28.7",
    "dotenv-webpack": "^1.5.4",
    "enzyme": "^2.9.1",
    "eslint": "^4.6.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.0.2",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "extract-css-chunks-webpack-plugin": "^2.0.17",
    "favicons-webpack-plugin": "^0.0.7",
    "inspectpack": "^1.3.2",
    "jest": "^21.0.2",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "postcss-nested": "^2.1.2",
    "react-test-renderer": "^15.6.1",
    "sinon": "^3.2.1",
    "static-site-generator-webpack-plugin": "^3.4.1",
    "webpack": "3.5.5",
    "webpack-dashboard": "^1.0.0-5",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.19.1",
    "webpack-hot-server-middleware": "^0.1.0",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "animated-transition-group": "^1.0.22",
    "babel-plugin-universal-import": "^1.2.7",
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.5",
    "debug": "^3.0.1",
    "dotenv": "^4.0.0",
    "express": "^4.15.4",
    "history": "^4.7.2",
    "imagesloaded": "^4.1.3",
    "immutable": "^3.8.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "next",
    "react-redux": "^5.0.6",
    "react-universal-component": "^2.5.4",
    "redbox-react": "^1.5.0",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-devtools-extension": "^2.13.2",
    "redux-first-router": "^1.9.19",
    "redux-first-router-link": "^1.4.1",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.15.6",
    "reselect": "^3.0.1",
    "sanitize.css": "^5.0.0",
    "store": "^2.0.12",
    "transit-immutable-js": "^0.7.0",
    "transit-js": "^0.8.846",
    "webpack-flush-chunks": "^1.1.23"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "source"
    ],
    "collectCoverageFrom": [
      "source/**/*.js"
    ],
    "moduleNameMapper": {
      "\\.(css|less|jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "identity-obj-proxy"
    }
  }
}
